{
  "name": "botium-speech-processing-frontend",
  "version": "1.0.0",
  "description": "Unified Speech Processing",
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "start-dev": "cross-env DOTENV_FLOW_PATH=./resources nodemon -w ./resources/.env -w ./resources/.env.local -w ./src/ -x \"node -r dotenv-flow/config\" ./src/server.js",
    "start-dist": "node -r dotenv-flow/config ./src/server.js",
    "jsdoc": "swagger-jsdoc -d ./src/swaggerDef.json -o ./src/swagger.json src/routes.js",
    "update-dependencies": "npm-check-updates -u --timeout 120000"
  },
  "author": "Botium GmbH",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-polly": "^3.776.0",
    "@aws-sdk/client-s3": "^3.775.0",
    "@aws-sdk/client-transcribe": "^3.775.0",
    "@aws-sdk/client-transcribe-streaming": "^3.775.0",
    "@aws-sdk/util-endpoints": "^3.775.0",
    "@google-cloud/speech": "^7.0.1",
    "@google-cloud/storage": "^7.15.2",
    "@google-cloud/text-to-speech": "^6.0.1",
    "body-parser": "^2.2.0",
    "cheerio": "^1.0.0",
    "content-disposition": "^0.5.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.4.0",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "express-winston": "^4.2.0",
    "ibm-watson": "^10.0.0",
    "lodash": "^4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "^1.43.0",
    "mkdirp": "^3.0.1",
    "multer": "^2.0.1",
    "mustache": "^4.2.0",
    "nodemon": "^3.1.9",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sanitize-filename": "^1.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "word-error-rate": "^0.0.7",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "npm-check-updates": "^17.1.16"
  }
}
